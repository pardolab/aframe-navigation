{
  "name": "aframe-navigation",
  "version": "1.0.1",
  "description": "Highly customizable navigation system",
  "main": "index.js",
  "cdn": "https://unpkg.com/aframe-navigation/dist/aframe-navigation.min.js",
  "scripts": {
    "dev": "budo  --port 7000 --live --open",
    "dist": "webpack index.js dist/aframe-navigation.js babelify && webpack -p index.js dist/aframe-navigation.min.js babelify",
    "lint": "semistandard -v | snazzy",
    "prepublish": "npm run dist",
    "ghpages": "ghpages",
    "start": "npm run dev",
    "watch": "gulp",
    "test": "karma start ./tests/karma.conf.js",
    "test:firefox": "karma start ./tests/karma.conf.js --browsers Firefox",
    "test:chrome": "karma start ./tests/karma.conf.js --browsers Chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pardolab/aframe-navigation.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "mozvr",
    "webvr",
    "navigation"
  ],
  "author": "Dominic Cicilio dpc2061@rit.edu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pardolab/aframe-navigation/issues"
  },
  "homepage": "https://github.com/pardolab/aframe-navigation#readme",
  "devDependencies": {
    "aframe": "*",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "budo": "^8.2.2",
    "chai": "^3.4.1",
    "chai-shallow-deep-equal": "^1.3.0",
    "ghpages": "^0.0.8",
    "gulp": "^3.9.1",
    "gulp-watch": "^4.3.11",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-chai-shallow-deep-equal": "0.0.4",
    "karma-chrome-launcher": "2.0.0",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-sinon-chai": "^1.1.0",
    "mocha": "^2.3.4",
    "randomcolor": "^0.4.4",
    "semistandard": "^8.0.0",
    "shelljs": "^0.7.0",
    "shx": "^0.1.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "snazzy": "^4.0.0",
    "webpack": "^3.4.1"
  },
  "semistandard": {
    "globals": [
      "AFRAME",
      "THREE"
    ],
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  },
  "dependencies": {
    "superagent": "^3.5.2"
  }
}
